#!/usr/bin/env bash

# black
readonly color_0='#282828'
readonly color_8='#504945'

# red
readonly color_1='#cc241d'
readonly color_9='#fb4934'

# green
readonly color_2='#98971a'
readonly color_10='#b8bb26'

# yellow
readonly color_3='#d79921'
readonly color_11='#fabd2f'

# blue
readonly color_4='#458588'
readonly color_12='#83a598'

# magenta
readonly color_5='#b16286'
readonly color_13='#d3869b'

# cyan
readonly color_6='#689d6a'
readonly color_14='#8ec07c'

# white
readonly color_7='#a89984'
readonly color_15='#ebdbb2'

function get_color_black() {
    echo "${color_0}"
}
function get_color_darkgrey() {
    echo "${color_8}"
}
function get_color_darkgray() {
    echo "${color_8}"
}

function get_color_darkred() {
    echo "${color_1}"
}
function get_color_red() {
    echo "${color_9}"
}

function get_color_darkgreen() {
    echo "${color_2}"
}
function get_color_green() {
    echo "${color_10}"
}

function get_color_darkyellow() {
    echo "${color_3}"
}
function get_color_orange() {
    echo "${color_3}"
}
function get_color_yellow() {
    echo "${color_11}"
}

function get_color_darkblue() {
    echo "${color_4}"
}
function get_color_blue() {
    echo "${color_12}"
}

function get_color_darkmagenta() {
    echo "${color_5}"
}
function get_color_magenta() {
    echo "${color_13}"
}

function get_color_darkcyan() {
    echo "${color_6}"
}
function get_color_cyan() {
    echo "${color_14}"
}

function get_color_lightgrey() {
    echo "${color_7}"
}
function get_color_lightgray() {
    echo "${color_7}"
}
function get_color_white() {
    echo "${color_15}"
}
